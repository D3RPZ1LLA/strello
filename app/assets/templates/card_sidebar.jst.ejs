<ul>
  <li>
    <h4>Members</h4>
    <ul class="members">
      <% card.participants.forEach(function(member) { %>
      <li data-id="<%= member.id %>">
				<%= JST['avatar']({ user: member }) %>
      </li>
      <% }) %>
    </ul>
		<div id="new-participations">
	    <button id="assign-member-button" class="menu-button sidebar group">
				<div class="member-icon-white">
					<img alt="People_head" src="/assets/people_head.png">
				</div>
				<div class="member-icon-blue">
					<img alt="People_head_blue" src="/assets/people_head_blue.png">
				</div>
				<span>
					Assign...
				</span>
			</button>
			<%= JST['new_participation']({ members: members }) %>
		</div>
  </li>

  <li>
    <h4>Actions</h4>
    <ul>
      <li>
        <button class="add-checklist-button sidebar">Add checklist...</button>
      </li>
    </ul>
  </li>
</ul>

