<nav class="login">
	<ul class="group">
		<% if logged_in? %>
		<li><%= link_to "#{current_user.email}", root_url %></li>
		<li><%= button_to "Logout", session_url, method: :delete, class: "logout" %></li>
		<% else %>
		<li><%= link_to "Sign Up", new_user_url %></li>
		<li><%= link_to "Login", new_session_url %></li>
		<% end %>
	</ul>
</nav>