<%= render partial: "board_header", locals: { board: @board } %>

<%= button_to "Finished", board_finished_url(@board), method: "get",  class: "board-button" %>
<article class="card-set">
  <ul>
<%= render @board.cards.select { |card| card.start_date < Time.now && card.due_date > Time.now} %>
  </ul>
</article>
<%= button_to "Pending", board_pending_url(@board), method: "get", class: "board-button" %>