<h1><%= @board.title %></h1>
<article>
  <div>
    <ul>
      <% @board.cards.each do |card| %>
        <% if card.due_date < Time.now %>
        <li class="card"><%= card.title %></li>
        <% end %>
      <% end %>
    </ul>
  </div>
</artile>
<ul>
  <li><%= link_to "Current", board_url(@board) %></li>
  <li><%= link_to "Pending", board_pending_url(@board) %></li>
</ul>
